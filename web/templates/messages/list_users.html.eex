<%= component("admin_message_nav", [conn: @conn]) %>
<div class="mt4 w-80 center">
  <%= for user <- @users do %>
    <%= link(to: sa_messages_path(@conn, :view_messages, %{user: user.id})) do %>
      <div class="">
        <p class="b"><%= user.id %></p>
      </div>
    <% end %>
  <% end %>
  <%= link(to: sa_super_admin_path(@conn, :index)) do %>
    <div class="">
      <p class="b">Back to main menu</p>
    </div>
  <% end %>
</div>
